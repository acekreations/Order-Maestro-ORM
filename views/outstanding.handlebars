<div class="row mb-5">
  <div class="col-3 border-right">
    {{> sideNav active}}
  </div>
  <div class="col-9">
    <div class="tab-content" id="v-pills-tabContent">
      <div class="tab-pane fade show active" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
        {{#each orders}}
        <div id="{{this.id}}" class="card mb-4 shadow border-0">
          <div class="card-body px-4">
            <div class="d-flex justify-content-between align-items-center mb-4">
              <h2 class="card-title"># {{this.id}}</h2>
              <button type="button" class="completeOrder btn btn-success" data-orderId="{{this.id}}"><i class="fas fa-check fa-2x"></i><br>Complete</button>
            </div>
            <table class="table table-striped">
              <tbody>
                {{#each items}}
                <tr>
                  <td>{{this}}</td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
        </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
